<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <PublishProtocol>FileSystem</PublishProtocol>
    <SelfContained>true</SelfContained>
    <WindowsAppSdkSelfContained>true</WindowsAppSdkSelfContained>
    <PublishReadyToRun>true</PublishReadyToRun>
    <PublishTrimmed>true</PublishTrimmed>
    <TrimMode>partial</TrimMode>
    <WindowsPackageType>None</WindowsPackageType>
    <PublishAot>true</PublishAot>
  </PropertyGroup>

  <PropertyGroup Condition="'$(OptimizationPreference)'=='speed'">
    <PublishDir>bin\$(RuntimeIdentifier)-aot-speed\</PublishDir>
  </PropertyGroup>

  <PropertyGroup Condition="'$(OptimizationPreference)'=='size'">
    <PublishDir>bin\$(RuntimeIdentifier)-aot-size\</PublishDir>
  </PropertyGroup>
</Project>